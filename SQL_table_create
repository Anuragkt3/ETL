CREATE TABLE EMPLOYEES (
    Id varchar(255),
    Name varchar(255),
    City varchar(255),
	Email varchar(255),
    Designation varchar(255),
	PhoneNumber varchar(255)
);

Insert Table 
========================
INSERT INTO MyTable
  ( Id, Name, Email )
VALUES
  (1,'John', 'Lloyds Office'), 
  (2,'Jane', 'Lloyds Office'), 
  (3,'Billy', 'London Office'),
  (4,'Miranda', 'Bristol Office');
